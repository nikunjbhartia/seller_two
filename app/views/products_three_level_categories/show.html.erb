<%- model_class = ProductsThreeLevelCategory -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:product_id) %>:</strong></dt>
  <dd><%= @products_three_level_category.product_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:third_level_id) %>:</strong></dt>
  <dd><%= @products_three_level_category.third_level_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              products_three_level_categories_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_products_three_level_category_path(@products_three_level_category), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              products_three_level_category_path(@products_three_level_category),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
